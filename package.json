  "bugs": {
    "url": "https://github.com/benowusulink/abbey-road-client/issues"
  },
  "homepage": "https://github.com/benowusulink/abbey-road-client",
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "parcel": "^2.15.4",
    "prettier": "3.6.2"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --config ./utilities/prettier/.prettierrc --ignore-path ./utilities/prettier/.prettierignore --write ./"
